#!/bin/sh /etc/rc.common
START=95
USE_PROCD=1
PROG="/usr/bin/msm8916-usb-gadget"

start_service() {
	procd_open_instance
	procd_set_param command /bin/sh $PROG start
	procd_set_param stdout 1
	procd_set_param stderr 1
	procd_close_instance
}

stop_service() {
	/bin/sh $PROG stop 2>/dev/null || true
}
