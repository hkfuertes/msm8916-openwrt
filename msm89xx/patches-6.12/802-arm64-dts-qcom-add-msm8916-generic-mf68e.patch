Index: linux-6.12.51/arch/arm64/boot/dts/qcom/msm8916-generic-mf68e.dts
===================================================================
--- /dev/null
+++ linux-6.12.51/arch/arm64/boot/dts/qcom/msm8916-generic-mf68e.dts
@@ -0,0 +1,293 @@
+// SPDX-License-Identifier: GPL-2.0-only
+
+/dts-v1/;
+
+#include "msm8916-ufi.dtsi"
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/leds/common.h>
+
+/ {
+	model = "MF68E 4G LTE Mobile WiFi Router";
+	compatible = "generic,mf68e", "qcom,msm8916";
+
+	bat: battery {
+		compatible = "simple-battery";
+		voltage-min-design-microvolt = <3400000>;
+		voltage-max-design-microvolt = <4350000>;
+		energy-full-design-microwatt-hours = <11100000>;
+		charge-full-design-microamp-hours = <3000000>;
+
+		ocv-capacity-celsius = <25>;
+		ocv-capacity-table-0 = <4330000 100>, <4265000 95>,
+			<4208000 90>, <4154000 85>, <4102000 80>,
+			<4063000 75>, <4006000 70>, <3972000 65>,
+			<3931000 60>, <3880000 55>, <3841000 50>,
+			<3817000 45>, <3798000 40>, <3783000 35>,
+			<3767000 30>, <3747000 25>, <3729000 20>,
+			<3700000 16>, <3687000 13>, <3683000 11>,
+			<3681000 10>, <3680000 9>, <3678000 8>,
+			<3674000 7>, <3666000 6>, <3641000 5>,
+			<3597000 4>, <3537000 3>, <3457000 2>,
+			<3336000 1>, <3000000 0>;
+	};
+
+	usb_id: usb-id {
+		compatible = "linux,extcon-usb-gpio";
+		id-gpios = <&tlmm 110 GPIO_ACTIVE_HIGH>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&usb_id_default>;
+	};
+
+	backlight: backlight {
+		compatible = "gpio-backlight";
+		gpios = <&tlmm 98 GPIO_ACTIVE_HIGH>;
+		default-on;
+	};
+
+	gpio-keys {
+		compatible = "gpio-keys";
+		pinctrl-names = "default";
+		pinctrl-0 = <&button_default>;
+
+		label = "GPIO Buttons";
+
+		button-wps {
+			label = "WPS";
+			gpios = <&tlmm 27 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_WPS_BUTTON>;
+			debounce-interval = <15>;
+		};
+
+		button-reset {
+			label = "Reset";
+			gpios = <&tlmm 102 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_RESTART>;
+			debounce-interval = <15>;
+		};
+	};
+
+	leds {
+		compatible = "gpio-leds";
+		pinctrl-names = "default";
+		pinctrl-0 = <&led_default>;
+
+		led-bat-1 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_CHARGING;
+			gpios = <&tlmm 1 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		led-bat-2 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_CHARGING;
+			gpios = <&tlmm 0 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		led-bat-3 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_CHARGING;
+			gpios = <&tlmm 91 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		led-bat-4 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_CHARGING;
+			gpios = <&tlmm 100 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		led-wifi {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_WLAN;
+			gpios = <&tlmm 73 GPIO_ACTIVE_HIGH>;
+			linux,default-trigger = "phy0tx";
+		};
+
+		led-4g-1 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_WAN;
+			gpios = <&tlmm 84 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		led-4g-2 {
+			color = <LED_COLOR_ID_GREEN>;
+			function = LED_FUNCTION_WAN;
+			gpios = <&tlmm 85 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+	};
+
+	esim-control {
+		compatible = "gpio-leds";
+		pinctrl-names = "default";
+		pinctrl-0 = <&esim_default>;
+
+		esim1 {
+			gpios = <&tlmm 119 GPIO_ACTIVE_HIGH>;
+			default-state = "on";
+		};
+
+		esim2 {
+			gpios = <&tlmm 112 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+
+		esim3 {
+			gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+	};
+
+	sim-detect {
+		compatible = "gpio-keys";
+		pinctrl-names = "default";
+		pinctrl-0 = <&sim_default>;
+
+		sim-hotplug {
+			label = "SIM Hotplug";
+			gpios = <&tlmm 114 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_UNKNOWN>;
+			linux,input-type = <EV_SW>;
+		};
+	};
+
+	modem-detect {
+		compatible = "gpio-keys";
+
+		modem-type {
+			gpios = <&tlmm 72 GPIO_ACTIVE_HIGH>;
+			linux,code = <KEY_UNKNOWN>;
+		};
+	};
+};
+
+&blsp_spi5 {
+	status = "okay";
+
+	panel@0 {
+		compatible = "sitronix,st7735r";
+		reg = <0>;
+		spi-max-frequency = <16000000>;
+
+		reset-gpios = <&tlmm 25 GPIO_ACTIVE_LOW>;
+		dc-gpios = <&tlmm 30 GPIO_ACTIVE_HIGH>;
+		backlight = <&backlight>;
+
+		width = <128>;
+		height = <128>;
+		rotation = <0>;
+
+		vdd-supply = <&pm8916_l17>;
+		vddio-supply = <&pm8916_l6>;
+
+		pinctrl-names = "default", "sleep";
+		pinctrl-0 = <&panel_default>;
+		pinctrl-1 = <&panel_sleep>;
+	};
+};
+
+&pm8916_bms {
+	status = "okay";
+	monitored-battery = <&bat>;
+	power-supplies = <&pm8916_charger>;
+};
+
+&pm8916_charger {
+	status = "okay";
+	monitored-battery = <&bat>;
+	qcom,fast-charge-safe-current = <900000>;
+	qcom,fast-charge-safe-voltage = <4350000>;
+};
+
+&rpm_requests {
+	pm8916-regulators {
+		pm8916_l17: l17 {
+			regulator-min-microvolt = <2850000>;
+			regulator-max-microvolt = <2850000>;
+		};
+	};
+};
+
+&usb {
+	status = "okay";
+	extcon = <&usb_id>, <&usb_id>;
+};
+
+&usb_hs_phy {
+	extcon = <&usb_id>;
+};
+
+&tlmm {
+	button_default: button-default-state {
+		pins = "gpio27", "gpio102";
+		function = "gpio";
+		drive-strength = <2>;
+		bias-pull-up;
+	};
+
+	esim_default: esim-default-state {
+		pins = "gpio12", "gpio112", "gpio119";
+		function = "gpio";
+		drive-strength = <2>;
+		bias-disable;
+	};
+
+	led_default: led-default-state {
+		pins = "gpio0", "gpio1", "gpio72", "gpio73", "gpio84", "gpio85", "gpio91", "gpio100";
+		function = "gpio";
+		drive-strength = <2>;
+		bias-disable;
+	};
+
+	panel_default: panel-default-state {
+		dc-pins {
+			pins = "gpio30";
+			function = "gpio";
+			drive-strength = <8>;
+			bias-disable;
+		};
+
+		enable-pins {
+			pins = "gpio97";
+			function = "gpio";
+			drive-strength = <8>;
+			bias-disable;
+			output-high;
+		};
+
+		reset-pins {
+			pins = "gpio25";
+			function = "gpio";
+			drive-strength = <8>;
+			bias-disable;
+			output-high;
+		};
+	};
+
+	panel_sleep: panel-sleep-state {
+		pins = "gpio25", "gpio30", "gpio97";
+		function = "gpio";
+		drive-strength = <2>;
+		bias-pull-down;
+		output-low;
+	};
+
+	sim_default: sim-default-state {
+		pins = "gpio114";
+		function = "gpio";
+		drive-strength = <2>;
+		bias-pull-up;
+	};
+
+	usb_id_default: usb-id-default-state {
+		pins = "gpio110";
+		function = "gpio";
+		drive-strength = <8>;
+		bias-pull-up;
+	};
+};
